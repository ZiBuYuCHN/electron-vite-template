{
  "name": "electron-vite-template",
  "version": "1.0.0",
  "main": "./dist/electron/main/main.js",
  "author": "sky <https://github.com/umbrella22>",
  "description": "electron-vite-template project",
  "license": "MIT",
  "scripts": {
    "dev": "tsx .electron-vite/dev-runner.ts",
    "build": "tsx .electron-vite/build.ts -m prod && electron-builder -c build.json",
    "build:win32": "tsx .electron-vite/build.ts  && electron-builder -c build.json --win  --ia32",
    "build:win64": "tsx .electron-vite/build.ts  && electron-builder -c build.json --win  --x64",
    "build:mac": "tsx .electron-vite/build.ts  && electron-builder -c build.json --mac",
    "build:dir": "tsx .electron-vite/build.ts  && electron-builder -c build.json --dir",
    "build:clean": "tsx .electron-vite/build.ts --clean",
    "build:web": "tsx .electron-vite/build.ts --target web",
    "pack:resources": "tsx .electron-vite/hot-updater.ts",
    "dep:upgrade": "yarn upgrade-interactive --latest",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "electron-log": "^5.2.0",
    "electron-updater": "^6.3.4",
    "glob": "^10.3.12",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "adm-zip": "^0.5.10",
    "axios": "^1.7.7",
    "cfonts": "^3.2.0",
    "chalk": "5.3.0",
    "del": "^8.0.0",
    "dotenv": "^16.4.5",
    "electron": "^32.1.0",
    "electron-builder": "^25.1.8",
    "electron-devtools-vendor": "^3.0.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^12.1.0",
    "javascript-obfuscator": "^4.1.0",
    "listr2": "^8.2.5",
    "minimist": "^1.2.8",
    "pinia": "^2.2.6",
    "portfinder": "^1.0.32",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-obfuscator": "^1.1.0",
    "sass": "^1.81.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.1",
    "typescript": "^5.7.2",
    "vite": "^5.4.6",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5"
  },
  "keywords": [
    "vite",
    "electron",
    "vue3",
    "rollup"
  ]
}
